{
  "name": "workos-migration-toolkit",
  "version": "2.0.0",
  "private": true,
  "description": "Complete migration toolkit for WorkOS User Management with Auth0 export, validation, and MCP support",
  "type": "module",
  "bin": {
    "workos-import-users": "bin/import-users.ts",
    "workos-export-auth0": "bin/export-auth0.ts",
    "workos-setup-auth0-test": "bin/setup-auth0-test-orgs.ts"
  },
  "scripts": {
    "start": "tsx bin/import-users.ts",
    "import:example": "tsx bin/import-users.ts --csv examples/example-input.csv",
    "typecheck": "tsc --noEmit",
    "test": "tsx scripts/run-all-tests.ts"
  },
  "engines": {
    "node": ">=18.17"
  },
  "dependencies": {
    "@workos-inc/node": "^7.73.0",
    "auth0": "^4.0.0",
    "chalk": "^5.6.2",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.4.5",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.7.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
