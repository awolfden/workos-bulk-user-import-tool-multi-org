{
  "name": "custom",
  "description": "Custom profile template - modify for your provider",
  "notes": [
    "This is a template for creating custom mapping profiles.",
    "Modify the mappings array to match your provider's CSV format.",
    "See docs/PHASE3-MAP.md for detailed documentation."
  ],
  "mappings": [
    {
      "sourceField": "email",
      "targetField": "email",
      "transformer": "lowercase_trim"
    },
    {
      "sourceField": "firstName",
      "targetField": "first_name",
      "transformer": "trim"
    },
    {
      "sourceField": "lastName",
      "targetField": "last_name",
      "transformer": "trim"
    },
    {
      "sourceField": "emailVerified",
      "targetField": "email_verified",
      "transformer": "to_boolean"
    },
    {
      "sourceField": "userId",
      "targetField": "external_id",
      "transformer": "trim"
    },
    {
      "sourceField": "organizationId",
      "targetField": "org_external_id",
      "transformer": "trim",
      "skipIfBlank": true
    },
    {
      "sourceField": "organizationName",
      "targetField": "org_name",
      "transformer": "trim",
      "skipIfBlank": true
    }
  ],
  "metadataMapping": {
    "targetField": "metadata",
    "sourceFields": ["customMetadata"],
    "fieldPrefix": "custom_",
    "staticMetadata": {
      "_provider": "custom"
    }
  }
}
